					"configurations": {
						"development": {
							"outputPath": "dist/development/server",
							"optimization": true,
							"outputHashing": "media",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.development.ts" }]
						},
						"staging": {
							"outputPath": "dist/staging/server",
							"optimization": true,
							"outputHashing": "media",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.staging.ts" }]
						},
						"production": {
							"outputPath": "dist/production/server",
							"optimization": true,
							"outputHashing": "media",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.production.ts" }]
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"projects/client/src/tsconfig.browser.json",
							"projects/client/src/tsconfig.server.json",
							"projects/client/src/tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		},
		"e2e": {
			"root": "projects/client-e2e/",
			"projectType": "application",
			"prefix": "",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "projects/client-e2e/protractor.conf.js",
						"devServerTarget": "client:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "client:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "projects/client-e2e/tsconfig.e2e.json",
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		}
	},
	"defaultProject": "browser",
	"schematics": {
		"@schematics/angular:component": {
			"styleext": "scss"
		}
	}
}
