		"browser": {
			"root": "projects/client/",
			"sourceRoot": "projects/client/src",
			"projectType": "application",
			"prefix": "app",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/development/browser",
						"index": "projects/client/src/index.html",
						"main": "projects/client/src/main/main-browser.bootstrap.ts",
						"polyfills": "projects/client/src/polyfills.ts",
						"tsConfig": "projects/client/tsconfig.browser.json",
						"assets": ["projects/client/src/favicon.ico", "projects/client/src/assets"],
						"styles": ["projects/client/src/styles.scss"],
						"extractCss": true,
						"lazyModules": [
							"projects/client/src/app/bundles/editor.bundle"
						],
						"scripts": [
							{ "input": "./node_modules/jquery/dist/jquery.js", "lazy": false },
							{ "input": "./node_modules/@fancyapps/fancybox/dist/jquery.fancybox.js", "lazy": false },
							{ "input": "./node_modules/intersection-observer/intersection-observer.js", "lazy": false },
							{ "input": "./node_modules/json-formatter-js/dist/json-formatter.js", "lazy": true },
							{ "input": "./node_modules/mapbox-gl/dist/mapbox-gl.js", "lazy": false },
							{ "input": "./node_modules/marked/lib/marked.js", "lazy": false }
						]
					},
					"configurations": {
						"development": {
							"outputPath": "dist/development/browser",
							"optimization": false,
							"outputHashing": "all",
							"sourceMap": true,
							"extractCss": true,
							"namedChunks": false,
							"aot": false,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": false,
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.development.ts" }],
							"serviceWorker": true
						},
						"staging": {
							"outputPath": "dist/staging/browser",
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.staging.ts" }],
							"serviceWorker": true
						},
						"production": {
							"outputPath": "dist/production/browser",
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.production.ts" }],
							"serviceWorker": true
						},
						"docs": {
							"baseHref": "/designr/",
							"deployUrl": "/designr/",
							"fileReplacements": [{ "replace": "projects/client/src/environments/environment.ts", "with": "projects/client/src/environments/environment.docs.ts" }],
							"optimization": true,
							"outputPath": "docs",
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"serviceWorker": false,
							"budgets": [{ "type": "initial", "maximumWarning": "2mb", "maximumError": "5mb" }]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "browser:build",
						"port": 50000,
						"proxyConfig": "proxy.conf.json"
					},
					"configurations": {
						"production": {
							"browserTarget": "browser:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "browser:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/client/src/test.ts",
						"polyfills": "projects/client/src/polyfills.ts",
						"tsConfig": "projects/client/tsconfig.spec.json",
						"karmaConfig": "projects/client/karma.conf.js",
						"styles": ["projects/client/src/styles.scss"],
						"scripts": [],
						"assets": ["projects/client/src/favicon.ico", "projects/client/src/assets"]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": ["projects/client/tsconfig.browser.json", "projects/client/tsconfig.server.json", "projects/client/tsconfig.spec.json"],
						"exclude": ["**/node_modules/**"]
					}
				}
			}
		},
		"server": {
