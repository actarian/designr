{
	"name": "designr",
	"version": "0.0.11",
	"keywords": [
    "designr",
    "angular",
    "framework"
  ],
	"repository": {
		"type": "git",
		"url": "https://github.com/actarian/designr.git"
	},
	"private": true,
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"build:browser:development": "ng build browser --configuration=development",
		"build:browser:staging": "ng build browser --configuration=staging",
		"build:browser:production": "ng build browser --configuration=production",
		"build:server:development": "ng build server --configuration=development",
		"build:server:staging": "ng build server --configuration=staging",
		"build:server:production": "ng build server --configuration=production",
		"build:development": "npm run build:browser:development & npm run build:server:development",
		"build:staging": "npm run build:browser:staging & npm run build:server:staging",
		"build:production": "npm run build:browser:production & npm run build:server:production",
		"build:docs": "ng build browser --configuration=docs",
		"debug:development": "node -e \"require('./utils/debug.js')(require('./dist/development/server/main.js').default, 'https://development.labs.it/')\"",
		"debug:staging": "node -e \"require('./utils/debug.js')(require('./dist/staging/server/main.js').default, 'http://staging.site.it/')\"",
		"debug:production": "node -e \"require('./utils/debug.js')(require('./dist/production/server/main.js').default, 'https://www.site.it/')\"",
		"pubver": "node -e \"require('./utils/publish.js')('../projects/designr')\""
	},
	"dependencies": {
		"@angular/animations": "~7.2.8",
		"@angular/common": "~7.2.8",
		"@angular/compiler": "~7.2.8",
		"@angular/core": "~7.2.8",
		"@angular/forms": "~7.2.8",
		"@angular/http": "~7.2.8",
		"@angular/platform-browser-dynamic": "~7.2.8",
		"@angular/platform-browser": "~7.2.8",
		"@angular/platform-server": "~7.2.8",
		"@angular/pwa": "~0.13.4",
		"@angular/router": "~7.2.8",
		"@angular/service-worker": "~7.2.8",
		"@fancyapps/fancybox": "~3.5.6",
		"@nguniversal/aspnetcore-engine": "~7.1.0",
		"@nguniversal/common": "~7.1.0",
		"@nguniversal/module-map-ngfactory-loader": "~7.1.0",
		"aspnet-prerendering": "~3.0.1",
		"bootstrap": "~4.3.1",
		"core-js": "^2.5.4",
		"intersection-observer": "~0.5.1",
		"jquery": "~3.4.0",
		"json-formatter-js": "~2.2.1",
		"mapbox-gl": "~0.52.0",
		"marked": "~0.7.0",
		"ngx-markdown": "~7.1.3",
		"rxjs": "~6.3.3",
		"swiper": "~4.4.6",
		"tslib": "~1.10.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.13.0",
		"@angular-devkit/build-ng-packagr": "~0.13.4",
		"@angular/cli": "~7.3.4",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/language-service": "~7.2.0",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "~4.5.0",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-jasmine": "~1.1.2",
		"karma": "~4.0.0",
		"ng-packagr": "~4.7.1",
		"protractor": "~5.4.0",
		"spawn-command-with-kill": "^1.0.2",
		"ts-node": "~7.0.0",
		"tsickle": ">=0.29.0",
		"tslib": "~1.10.0",
		"tslint": "~5.11.0",
		"typescript": "~3.2.2",
		"webpack-bundle-analyzer": "^3.3.2"
	}
}
